{
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["vue"],
	"rules": {
		"indent": ["warn", "tab"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"vue/multi-word-component-names": "off"
	}
}
